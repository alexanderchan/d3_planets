    "cssnano": "^4.1.10",
    "d3": "3.5.16",
    "sass": "^1.23.3"
  },
  "dependencies": {
    "dotenv": "8.2.0",
    "react": "16.11.0",
    "react-dom": "^16.11.0",
    "rimraf": "3.0.0"
  }
